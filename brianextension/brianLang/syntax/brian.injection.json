{
  "scopeName": "brian.injection",
  "injectionSelector": "L:meta.function-call.python",
  "patterns": [
    {
      "include": "#injection1"
    },
    {
      "include": "#injection2"
    },
    {
      "include": "#injection3"
    }
  ],
  "repository": {
    "injection1": {
      "name": "meta.function-call.brian",
      "begin": "\\bEquations\\(\\s*'''\\s*",
      "patterns": [
        {
          "include": "source.brian"
        }
      ],
      "end": "'''\\)"
    },
    "injection2": {
      "name": "meta.function-call.brian",
      "begin": "\\bEquations\\(\\s*'\\s*",
      "patterns": [
        {
          "include": "source.brian"
        }
      ],
      "end": "'\\)"
    },
    "injection3": {
      "name": "meta.function-call.brian",
      "begin": "\\bEquations\\(\\s*\"\\s*",
      "patterns": [
        {
          "include": "source.brian"
        }
      ],
      "end": "\"\\s*\\)"
    },
    "injection4":{
      "name": "meta.function-call.brian",
      "begin": "\\bNeuronGroup\\(\\s*\\d+\\s*,\\s*'''\\s*",
      "patterns": [
        {
          "include": "source.brian"
        }
      ],
      "end": "''',\\s*threshold='([\\s\\S]*?)',\\s*reset='([\\s\\S]*?)'\\)"
    }
  }
}
